<div id="tasks_wall" class="span12"></div>
<script type="text/javascript">
  $(function() {
    // Blog is the app name
    window.router = new Miamir.Routers.DashboardRouter({ready_tasks: <%= @ready_tasks.to_json.html_safe -%>,progress_tasks: <%= @progress_tasks.to_json(:include => {:owner=> { :except => [:created_at, :updated_at]},:partner=> { :except => [:created_at, :updated_at]}}).html_safe -%>,done_tasks: <%= @done_tasks.to_json.html_safe -%>,current_user:<%=current_user.to_json({ :include =>{:teams=>{:except => [:created_at, :updated_at]}},:except => [:created_at, :updated_at]}).html_safe -%>});
    Backbone.history.start();
  });
</script>